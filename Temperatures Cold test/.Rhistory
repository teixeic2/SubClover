theme(axis.text.x=element_text(angle = 0, hjust = 0.5))+
#remove grid lines
theme(
panel.border = element_rect(colour = "black", size = 3),
panel.grid.major = element_blank(),
panel.grid.minor = element_blank(),
axis.line = element_line(colour = "black"))+
